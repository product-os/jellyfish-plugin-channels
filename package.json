{
  "name": "@balena/jellyfish-plugin-channels",
  "description": "Channels Jellyfish Plugin",
  "version": "1.1.360",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-channels.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run types",
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "types": "node ./scripts/generate-contract-interfaces && npm run lint:fix"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-logger": "^3.0.125",
    "@balena/jellyfish-plugin-base": "^2.1.285",
    "lodash": "^4.17.21",
    "slugify": "^1.6.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.8",
    "@balena/jellyfish-config": "^1.5.4",
    "@balena/jellyfish-core": "^8.1.0",
    "@balena/jellyfish-types": "^1.2.0",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^27.0.2",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^12.0.1",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^8.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-11-19T11:34:11.722Z"
  }
}
