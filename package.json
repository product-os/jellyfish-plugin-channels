{
  "name": "@balena/jellyfish-plugin-channels",
  "description": "Channels Jellyfish Plugin",
  "version": "1.1.25",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-channels.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --typescript --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "lint": "balena-lint --typescript lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=shellcheck,@types/jest",
    "lint-fix": "balena-lint --typescript --fix lib test",
    "unit": "jest",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-logger": "^1.0.57",
    "@balena/jellyfish-plugin-base": "^1.2.41",
    "lodash": "^4.17.21",
    "slugify": "^1.5.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-core": "^2.0.24",
    "@balena/lint": "^5.4.1",
    "@types/jest": "^26.0.21",
    "@types/sinon": "^9.0.11",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "shellcheck": "^1.0.0",
    "sinon": "^9.2.4",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
