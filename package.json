{
  "name": "@balena/jellyfish-plugin-channels",
  "description": "Channels Jellyfish Plugin",
  "version": "2.0.112",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-channels.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "types": "node ./scripts/generate-contract-interfaces && npm run lint:fix"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-logger": "^5.0.31",
    "@balena/jellyfish-worker": "^23.0.17",
    "lodash": "^4.17.21",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.6",
    "@balena/jellyfish-types": "^2.0.5",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.5.0",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "autumndb": "^19.1.41",
    "deplint": "^1.1.3",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^13.0.2",
    "ts-jest": "^28.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-05-09T13:13:20.693Z"
  }
