{
  "name": "@balena/jellyfish-plugin-channels",
  "description": "Channels Jellyfish Plugin",
  "version": "1.1.283",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-channels.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run types",
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint:fix": "balena-lint --fix lib test",
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "types": "node ./scripts/generate-contract-interfaces && npm run lint:fix"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-logger": "^3.0.66",
    "@balena/jellyfish-plugin-base": "^2.1.234",
    "lodash": "^4.17.21",
    "slugify": "^1.6.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.4.8",
    "@balena/jellyfish-core": "^5.5.0",
    "@balena/jellyfish-types": "^0.9.9",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^27.0.1",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "shellcheck": "^1.0.0",
    "simple-git-hooks": "^2.6.1",
    "sinon": "^11.1.2",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.21.8",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^5.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-09-03T14:43:40.212Z"
  }
